.footer-wrap{
  position relative
  bottom 0
  height 100px
  width 100%
  color: #3e3e3e
  display flex
  align-items center
  flex-direction column
  justify-content center
  font-size 0.6em
  a{
    text-decoration none
    color #3e3e3e
    &:clicked{
      color #fefdff
    }
    &:hover{
      color #17c3d2;
    }
  }
}
